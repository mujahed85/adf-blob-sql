{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceDS",
						"type": "DatasetReference"
					},
					"name": "MoviesSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "snk1960"
				},
				{
					"dataset": {
						"referenceName": "DSoviesafter1980",
						"type": "DatasetReference"
					},
					"name": "snk1980"
				},
				{
					"dataset": {
						"referenceName": "DSallothersmovies",
						"type": "DatasetReference"
					},
					"name": "snkOthers"
				}
			],
			"transformations": [
				{
					"name": "split1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movie as string,",
				"          title as string,",
				"          genres as string,",
				"          year as string,",
				"          Rating as string,",
				"          {Rotton Tomato} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesSource",
				"MoviesSource split(toInteger(year) < 1960,",
				"     toInteger(year) > 1980,",
				"     disjoint: false) ~> split1@(moviesBefore1960, moviesAfter1980, AllOthersMovies)",
				"split1@moviesBefore1960 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> snk1960",
				"split1@moviesAfter1980 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> snk1980",
				"split1@AllOthersMovies sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> snkOthers"
			]
		}
	}
}